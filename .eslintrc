{
	"env": {
		"browser": true,
		"es6": true,
		"jest": true
	},
	"extends": ["airbnb-base", "plugin:vue/base", "prettier"],
	"parser": "vue-eslint-parser",
	"parserOptions": {
		"parser": "babel-eslint",
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"rules": {
		"no-plusplus": 0,
		"no-param-reassign": 0,
		"vue/html-self-closing": "off",
		"import/no-unresolved": "off"
	},
	"plugins": ["vue", "import", "prettier"]
}
